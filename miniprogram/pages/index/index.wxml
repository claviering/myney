<!--index.wxml-->
<view class="container">
  <view class="header">
    <header bind:toggleLeftMenu="toggleLeftMenu" />
  </view>
  <view class="show-money">
    <showMoneyData
      dataObject="{{dataObject}}"
    />
  </view>
  <view class="footer">
    <footer />
  </view>
  <view class="left-menu {{showLeftMenu ? 'show-left-menu' : 'hidden-left-menu'}}">
    <leftMenu bind:selectTime="selectTime" />
  </view>
</view>
